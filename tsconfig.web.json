{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "outDir": "./public",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": [
    "src/web/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}